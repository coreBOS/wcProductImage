<name>vtiger_wcproductimage</name>
<sql><![CDATA[CREATE TABLE `vtiger_wcproductimage` (
  `wcproductimageid` int(11) NOT NULL,
  `wcproductimage_no` varchar(56) default NULL,
  `wcpiname` varchar(196) default NULL,
  `wcpialt` varchar(560) default NULL,
  `wcpistatus` varchar(65) default NULL,
  `wcpicaption` varchar(165) default NULL,
  `wcpimage` varchar(156) default NULL,
  `wcpirelated` int(11) default NULL,
  `wcpitype` varchar(65) default NULL,
  PRIMARY KEY (`wcproductimageid`),
  INDEX (`wcpiname`),
  INDEX (`wcpirelated`),
  INDEX (`wcpistatus`),
  INDEX (`wcpitype`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
